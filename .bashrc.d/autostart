# -----------------------------------------------------
# AUTOSTART
# -----------------------------------------------------

# -----------------------------------------------------
# Tmux
# -----------------------------------------------------
if [ -z "$TMUX" ]; then
    tmux attach-session || tmux new-session
fi

# -----------------------------------------------------
# Fastfetch
# -----------------------------------------------------
if [[ $(tty) == *"pts"* ]]; then
    fastfetch --config paleofetch.jsonc
fi
